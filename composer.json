{
    "name": "yurunsoft/tdengine-orm",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "yurunsoft/tdengine-restful-connector": "^1.0",
        "doctrine/annotations": "^1.13",
        "symfony/polyfill-php81": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7",
        "friendsofphp/php-cs-fixer": "^3.0.2",
        "phpstan/phpstan": "^1.0.2",
        "yurunsoft/ide-helper": "~1.0",
        "swoole/ide-helper": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Yurun\\TDEngine\\Orm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yurun\\TDEngine\\Orm\\Test\\": "tests/unit"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c ./tests/phpunit.xml"
    },
    "extra": {
        "ide-helper": {
            "list": [
                "tdengine"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "yurunsoft/ide-helper": true
        }
    }
}